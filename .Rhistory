library(dplyr)
library(tidyr)
library(tibble)
library(ggplot2)
wines = read.csv(
'./Wine_Train.csv',
header = TRUE
)
sprintf("Data composed of: %d rows and %d columns",
nrow(wines),
ncol(wines))
head(wines, n = 5)
emptyData <- colSums(is.na(wines))
totalRows <- nrow(wines)
tibble(Column = names(wines),
`NA Rows` = totalRows,
`NA Count` = emptyData)
tibble(Column = names(wines),
`NA_Rows` = totalRows,
`NA_Count` = emptyData)
tibble(Column = names(wines),
`Total_Rows` = totalRows,
`NA_Count` = emptyData)
selected_data <- wines %>%  select(-quality)
corr_matrix <- cor(selected_data, use = "complete.obs")
plot(corr_matrix)
plot_corr(corr_matrix)
corrplot(corr_matrix)
corr_matrix <- cor(selected_data, use = "complete.obs")
## Look at the data corr
as.data.frame(as.table(corr_matrix)) %>% filter(Var1 != "quality")  %>%
ggplot(aes(Var1, Var2, fill = Freq)) +
geom_tile(color = "white") +
geom_text(
aes(label = round(Freq, 2)),
color = "black",
angle = 90,
size = 5
) +
coord_fixed(ratio = 7) +
scale_fill_gradient2(
low = "#d73027",
high = "#17c3b2",
mid = "#ffffff",
midpoint = 0,
name = "Correlation",
limit = c(-1, 1)
) +
theme(axis.text.x = element_text(
angle = 90,
vjust = 0.5,
size = 14,
hjust = 1,
margin = margin(
t = 5,
b = 0,
l = 10
),
)) +
labs(
title = waiver(),
subtitle = waiver(),
x = "",
y = ""
)
corrplot(corr_matrix)
